<% if(isLoggedIn) {%>
    <%- include('../layouts/userLoggedHeader.ejs') %>
        <% }else{ %>
            <%- include('../layouts/userHeader.ejs') %>
                <%}%>
                    <section class="categories">
                        <div class="container-fluid">
                            <div class="row">
                                <div class="col-lg-6 p-0">
                                    <div class="categories__item categories__large__item set-bg"
                                        data-setbg="/users/assets/Home/img/smartphones2.jpeg"
                                        style="background-image: url('/users/assets/Home/img/smartphones4.jpg');">
                                        <div class="categories__text">
                                            <h1 class="text-light">Smart Phones</h1>
                                            <br>
                                            <a href="/shop" class="text-light">Shop now</a>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-lg-6">
                                    <div class="row">
                                        <div class="col-lg-6 col-md-6 col-sm-6 p-0">
                                            <div class="categories__item set-bg"
                                                data-setbg="/users/assets/Home/img/categories/category-2.jpg"
                                                style="background-image: url('/users/assets/Home/img/laptops1.jpg');">
                                                <div class="categories__text">
                                                    <h4 class="text-light">Laptops</h4>
                                                    <br>
                                                    <a class="text-light" href="/shop">Shop now</a>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="col-lg-6 col-md-6 col-sm-6 p-0">
                                            <div class="categories__item set-bg"
                                                data-setbg="/users/assets/Home/img/categories/category-3.jpg"
                                                style="background-image: url('/users/assets/Home/img/headphones1.jpg');">
                                                <div class="categories__text">
                                                    <h4 class="text-light">Headphones</h4>
                                                    <br>
                                                    <a class="text-light" href="/shop">Shop now</a>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="col-lg-6 col-md-6 col-sm-6 p-0">
                                            <div class="categories__item set-bg"
                                                data-setbg="/users/assets/Home/img/categories/category-4.jpg"
                                                style="background-image: url('/users/assets/Home/img/smartwatches1.jpg');">
                                                <div class="categories__text">
                                                    <h4 class="text-light">SmartWatches</h4>
                                                    <br>
                                                    <a class="text-light" href="/shop">Shop now</a>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="col-lg-6 col-md-6 col-sm-6 p-0">
                                            <div class="categories__item set-bg"
                                                data-setbg="/users/assets/Home/img/categories/category-5.jpg"
                                                style="background-image: url('/users/assets/Home/img/playstation.jpg');">
                                                <div class="categories__text">
                                                    <h4 class="text-light">Gaming Accessories </h4>
                                                    <br>
                                                    <a class="text-light" href="/shop">Shop now</a>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </section>
                    <!-- Categories Section End -->

                    <!-- Product Section Begin -->
                    <section class="product spad" style="background-color: white">
                        <div class="container">
                            <div class="row">
                                <div class="col-lg-4 col-md-4">
                                    <div class="section-title">
                                        <h4>New product</h4>
                                    </div>
                                </div>
                                <div class="col-lg-8 col-md-8">
                                    <ul class="filter__controls">
                                        <% if(category){ %>
                                            <% if(category.length>0){ %>
                                                <li class="active" data-filter="*">All</li>
                                                <%for(let i=0; i < category.length; i++) { %>
                                                    <li data-filter="">
                                                        <a href="/category-products?id=<%=category[i]._id%>"
                                                            class="text-dark">
                                                            <%= category[i].names %>
                                                        </a>
                                                    </li>
                                                    <%}%>
                                                        <% } }%>
                                    </ul>
                                </div>
                            </div>
                            <% if(products){ %>
                                <div class="row property__gallery">
                                    <% if(products.length>0) {%>
                                        <%for(let i=0; i < products.length; i++) { %>
                                            <div class="col-lg-4 col-md-4 col-sm-6">
                                                <div class="product__item">
                                                    <div class="product__item__pic">
                                                        <% if(products[i].image) {%>
                                                            <a href="/single-product?id=<%=products[i]._id%>"><img
                                                                    src="/banners/<%=products[i].image[0]%>"
                                                                    class="product__item__pic" alt=""></a>
                                                            <!-- <div class="label new">New</div> -->
                                                            <ul class="product__hover">
                                                                <li><a href="/banners/<%=products[i].image[0]%>"
                                                                        class="image-popup"><span
                                                                            class="arrow_expand"></span></a></li>
                                                                <li><a href="/addToWishlist?id=<%=products[i]._id%>"><span
                                                                            class="icon_heart_alt"></span></a></li>
                                                                <li><a href="/addToCart?id=<%=products[i]._id%>"><span
                                                                            class="icon_bag_alt"></span></a></li>
                                                                <% }else{%>
                                                                    Image Not found
                                                                    <%}%>
                                                            </ul>
                                                    </div>
                                                    <div class="product__item__text">
                                                        <h6>
                                                            <%=products[i].name%>
                                                        </h6>
                                                        <div class="rating">
                                                            <i class="fa fa-star"></i>
                                                            <i class="fa fa-star"></i>
                                                            <i class="fa fa-star"></i>
                                                            <i class="fa fa-star"></i>
                                                            <i class="fa fa-star"></i>
                                                        </div>
                                                        <div class="product__price">
                                                            ₹ <%=products[i].price %>/
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                            <% } }%>
                                </div>
                                <%}%>
                        </div>
                    </section>
                    <!-- Discount Section Begin -->
                    <section class="discount" style="background-color:#f4f4f4">
                        <div class="container">
                            <div>
                                <div class="col-lg-6 m-auto">
                                    <div class="discount__text">
                                        <div class="discount__text__title">
                                            <span>Discount</span>
                                            <h2>Winter 2023</h2>
                                            <h5><span>Sale</span> 50%</h5>
                                        </div>
                                        <div class="discount__countdown" id="countdown-time">
                                            <div class="countdown__item">
                                                <span>22</span>
                                                <p>Days</p>
                                            </div>
                                            <div class="countdown__item">
                                                <span>18</span>
                                                <p>Hour</p>
                                            </div>
                                            <div class="countdown__item">
                                                <span>46</span>
                                                <p>Min</p>
                                            </div>
                                            <div class="countdown__item">
                                                <span>05</span>
                                                <p>Sec</p>
                                            </div>
                                        </div>
                                        <a href="/shop">Shop now</a>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </section>
                    <!-- Discount Section End -->

                    <!-- Services Section Begin -->
                    <section class="services spad">
                        <div class="container">
                            <div class="row">
                                <div class="col-lg-3 col-md-4 col-sm-6">
                                    <div class="services__item">
                                        <i class="fa fa-car"></i>
                                        <h6>Free Shipping</h6>
                                        <p>For all oder over ₹5000</p>
                                    </div>
                                </div>
                                <div class="col-lg-3 col-md-4 col-sm-6">
                                    <div class="services__item">
                                        <i class="fa fa-money"></i>
                                        <h6>Money Back Guarantee</h6>
                                        <p>If You Return In 7Days</p>
                                    </div>
                                </div>
                                <div class="col-lg-3 col-md-4 col-sm-6">
                                    <div class="services__item">
                                        <i class="fa fa-support"></i>
                                        <h6>Online Support 24/7</h6>
                                        <p>Dedicated support</p>
                                    </div>
                                </div>
                                <div class="col-lg-3 col-md-4 col-sm-6">
                                    <div class="services__item">
                                        <i class="fa fa-headphones"></i>
                                        <h6>Payment Secure</h6>
                                        <p>100% secure payment</p>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </section>
                    <!-- Services Section End -->

