

        <%- include('../layouts/adminHeader.ejs') %>

        <div class="clearfix"></div>

        <div class="content-wrapper">
            <div class="container-fluid">

                <!--Start Dashboard Content-->

                <div class="content">
                    <div class="container-fluid">
                    <div class="row">
                        <div class="container-fluid">
                        <div class="row">
                            <div class="col-lg-6">
                            <div class="card">
                                <canvas id="myChart"></canvas>
                                <input type="hidden" value="<%=pName %>" id="pname" />
                                <input type="hidden" value="<%=pCount%>" id="pcount" />
                                </div>
                                <script src="https://cdn.jsdelivr.net/npm/chart.js"></script>
                                <script>
                                const ctx = document.getElementById("myChart");
                                const name = document.getElementById("pname");
                                const count = document.getElementById("pcount");
                                const pNames = name.value.split(",");
                                const pCounts = count.value.split(",");
              
                                new Chart(ctx, {
                                  type: "pie",
                                  data: {
                                    labels: pNames,
                                    datasets: [
                                      {
                                        label: "sales",
                                        data: pCounts,
                                        borderWidth: 1,
                                      },
                                    ],
                                  },
                                  options: {
                                    scales: {
                                      y: {
                                        beginAtZero: true,
                                      },
                                    },
                                  },
                                });
                              </script>
                            </div>
                            <div class="col-lg-6">
                              <div class="card">
                                <canvas id="myAnotherChart"></canvas>
                                <input type="hidden" value="<%=category %>" id="category" />
                                <input type="hidden" value="<%=count%>" id="count" />
                              </div>
              
                              <script src="https://cdn.jsdelivr.net/npm/chart.js"></script>
              
                              <script>
                                const cty = document.getElementById("myAnotherChart");
                                const catName = document.getElementById("category");
                                const catCount = document.getElementById("count");
                                const catNames = catName.value.split(",");
                                const catCounts = catCount.value.split(",");
              
                                new Chart(cty, {
                                  type: "bar",
                                data: {
                                    labels: catNames,
                                    datasets: [
                                    {
                                        label: "sales",
                                        data:pCounts,
                                        borderWidth: 1,
                                    },
                                    ],
                                },
                                options: {
                                    scales: {
                                    y: {
                                        beginAtZero: true,
                                    },
                                    },
                                },
                                });
                            </script>
                            </div>
                        </div>
                        </div>
                </div>
                    </div>
                </div>
                <!--End Row-->
                <!--End Dashboard Content-->
                <!--start overlay-->
                <div class="overlay toggle-menu"></div>
                <!--end overlay-->
            </div>
            <!-- End container-fluid-->
        </div>
        <%- include('../layouts/adminFooter.ejs') %> 
